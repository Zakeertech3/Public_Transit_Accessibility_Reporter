{
  "installCommand": "echo 'No Python dependencies to install'",
  "buildCommand": "echo 'Static frontend - no build needed'",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}